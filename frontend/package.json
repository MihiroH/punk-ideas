    "check": "npm run next:lint && npx @biomejs/biome check --write ./src"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@biomejs/cli-darwin-arm64": "1.8.0",
    "@biomejs/cli-darwin-x64": "1.8.0",
    "@biomejs/cli-linux-arm64": "1.8.0",
    "@biomejs/cli-linux-x64": "1.8.0",
    "@biomejs/cli-win32-arm64": "1.8.0",
    "@biomejs/cli-win32-x64": "1.8.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "typescript": "^5"
  }
}
